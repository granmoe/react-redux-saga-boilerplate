{
  "env": {
    "production": {
      "presets": [["es2015", { modules: false }], "react", "stage-1"]
    },
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-1"
      ],
      "plugins": ["babel-plugin-transform-es2015-parameters", "babel-plugin-transform-es2015-destructuring"]
    },
    "development": {
      "presets": [
        "es2015",
        "react-hmre",
        "react",
        "stage-1"
      ],
      "plugins": ["babel-plugin-transform-es2015-parameters", "babel-plugin-transform-es2015-destructuring"]
    }
  }
}
