{
  "name": "react-redux-saga-boilerplate",
  "version": "0.0.3",
  "description": "A boilerplate frontend repo that uses react, redux, redux saga, webpack and babel and makes as few other assumptions as possible",
  "main": "webpack.config.js",
  "author": "Matt Granmoe",
  "license": "MIT",
  "homepage": "https://github.com/granmoe/react-redux-saga-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/granmoe/react-redux-saga-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/granmoe/react-redux-saga-boilerplate/issues"
  },
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "test": "jest --verbose"
  },
  "betterScripts": {
    "start": {
      "command": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js -d --progress --colors",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --optimize-dedupe",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "0.0.8",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.25.0",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "imagemin-webpack-plugin": "^1.2.1",
    "immutable": "^3.8.1",
    "jest": "^17.0.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-json-tree": "^0.10.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^1.16.1",
    "whatwg-fetch": "^1.0.0"
  }
}
